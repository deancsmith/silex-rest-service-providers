{
    "name": "marcojanssen/silex-rest-service-providers",
    "description": "Package containing several services for silex/rest skeleton",
    "keywords": ["silex", "rest", "api", "provider"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Janssen",
            "email": "dev@marcojanssen.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "dflydev/doctrine-orm-service-provider": "1.0.*",
        "doctrine/orm": "2.4.*",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "2.0.*",
        "jms/serializer": "0.15.0",
        "jurjean/spray-persistence-bundle": "2.*",
        "silex/silex": "1.1.*",
        "symfony/finder": "~2.3",
        "symfony/process": "~2.3",
        "symfony/property-access": "~2.3",
        "symfony/validator": "~2.3",
        "zendframework/zend-filter": "2.*",
        "zendframework/zend-loader": "2.*",
        "zircote/swagger-php": "0.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "suggest": {
        "silex/rest": "This package is implemented by silex/rest"
    },
    "autoload": {
        "psr-0": {
            "MJanssen\\": "src/"
        }
    }
}
